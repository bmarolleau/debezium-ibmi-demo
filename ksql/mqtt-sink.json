{
  "name": "mqtt-sink",
  "config": {
    "connector.class": "io.lenses.streamreactor.connect.mqtt.sink.MqttSinkConnector",
    "tasks.max": "1",
    "topics": "orders_aggregated",
    "connect.mqtt.hosts": "tcp://mqtt-broker:1883",
    "connect.mqtt.client.id": "orders-sink",
    "connect.mqtt.clean": "true",
    "connect.mqtt.keep.alive": "1000",
    "connect.mqtt.service.quality": "1",
    "connect.mqtt.kcql": "INSERT INTO orders/aggregates SELECT * FROM orders_aggregated",
    "key.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "false"
  }
}
